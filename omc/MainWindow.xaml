<Window x:Name="MainWindows" x:Class="omc.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:src="clr-namespace:nms_usercontrol_libs.src;assembly=nms_usercontrol_libs" 
        Title="NMS Login" Height="460" Width="660" WindowStartupLocation="CenterScreen" WindowStyle="None"  Icon="images/earth_48.ico" AllowsTransparency="True" Background="{x:Null}">
    <Grid Width="660" Height="460">        
        <src:UserLogin x:Name="UserLoginView" Margin="0" MouseLeftButtonDown="UserLoginView_MouseLeftButtonDown" >
            <src:UserLogin.Effect>
                <DropShadowEffect BlurRadius="30" ShadowDepth="10"/>
            </src:UserLogin.Effect>
        </src:UserLogin>
    </Grid>
</Window>
